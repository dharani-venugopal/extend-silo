import java.util.*;
public class sumofdigits {
	public static void main(String args[]){
		System.out.println(" enter the number");
		Scanner cc=new Scanner(System.in);
		int value= cc.nextInt();
		int temp=0;
		while(value!=0){
			int rem =value %10;
			temp=temp+rem;
			value=value/10;
			
		}
		System.out.println("sum of the digits"+temp);
		
	}

}
